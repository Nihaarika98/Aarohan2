<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="aarohan.nihaarika.aarohan">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=" "
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:name=".BNMIT.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="MBA-BNM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AArohan.Splash_Screen"
            android:theme="@style/Theme.Design.NoActionBar">

        </activity>
        <activity
            android:name=".AArohan.Event"
            android:parentActivityName=".BNMIT.MainActivity" />
        <activity
            android:name=".AArohan.NavigationActivity"
            android:theme="@style/Theme.Design.NoActionBar" />
        <activity android:name=".AArohan.Inaugural"></activity>
    </application>

</manifest>